{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/svelte/*": [
        "src/components/islands/svelte/*"
      ],
      "@/layouts/*": [
        "src/components/layouts/*"
      ],
      "@/stores/*": [
        "src/stores/*"
      ],
      "@/helpers/*": [
        "src/helpers/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@lucide/preact": [
        "node_modules/lucide-preact"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}